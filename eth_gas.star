# Get from etherscan.io/myapikey
ETHERSCAN_API_KEY="" 


#########################################################
## END OF CONFIGURATION AREA ############################
## EDIT THE CODE BELOW AT YOUR OWN RISK #################
#########################################################

load("encoding/base64.star", "base64")
load("render.star", "render")
load("http.star", "http")

SLOW_ICON = base64.decode("")
AVERAGE_ICON = base64.decode("")
FAST_ICON = base64.decode("")

def main():

    response = http.get("https://api.etherscan.io/api?module=gastracker&action=gasoracle&apikey="+ETHERSCAN_API_KEY)
    
    if response.status_code != 200:
        fail("Could not retrieve gas data  %d", response.status_code)

    result = response.json()['result']

    slow    = result['SafeGasPrice']
    average = result['ProposeGasPrice']
    fast    = result['FastGasPrice']

    return render.Root(

        render.Box(
            render.Column(
                expanded = True,
                main_align = "space_evenly",
                children = [
                    render.Row(
                        children = [
                            render.Image(src=SLOW_ICON),
                            render.Text(" " + slow + " gwei")
                        ]
                    ),
                    render.Row(
                        children = [
                            render.Image(src=AVERAGE_ICON),
                            render.Text(" " + average + " gwei")
                        ]
                    ),
                    render.Row(
                        children = [
                            render.Image(src=FAST_ICON),
                            render.Text(" " + fast + " gwei")
                        ]
                    ),
                ]
            )
        )
    )